	    <set name="bot1_name">Superdupont</set>
	    <set name="bot1">klown 5 blue 40</set>
	    <set name="bot2_name">Golem</set>
	    <set name="bot2">jenny 5 red 110</set>
	    <set name="bot3_name">DevilMan</set>
	    <set name="bot3">ohplease 5 blue 80</set>
	    <set name="bot4_name">ElMatador</set>
