<configuration plugin="bothandler">
	<settings name="settings">
		 <!-- Edit this to indicate what B3 user level can use commands for this plugin -->
		 <set name="admin_level">20</set>
		 <!-- How many bots to use? (Max: 8)-->
	<set name="bot_amount">6</set>
   </settings>
	<!-- Add bots below. Usage: <bot name> <level> <team> <ping> <alias>.
	Example: python 5 blue 110 =lvl5=python -->
